{
    "manifest_version": 2,
  
    "name": "Event Awareness Pluggin",
    "description": "This extension runs Python code.",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "https://ajax.googleapis.com/"
    ],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["bundle.js", "reddit.js"]
      }
    ]
  }
  