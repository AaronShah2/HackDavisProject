{
    "manifest_version": 2,
  
    "name": "BrightSide",
    "description": "An extension to improve your mood.",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "https://ajax.googleapis.com/"
    ],
    "background": {
      "scripts": ["bundle.js"]
    }
  }
  